{
    "extends": "./tsconfig.base.json",
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "outDir": "./dist/cjs"
    },
    "exclude": [
        "node_modules",
        "dist"
    ],
    "include": [
        "./src/**/*.ts",
        "./test"
    ],
    "paths": {
        "@microsoft/kiota-abstractions": [
            "../abstractions"
        ],
        "@microsoft/kiota-serialization-json": [
            "../serialization/json"
        ],
        "msgraph-javascript-sdk": [
            "../serviceLib"
        ],
        "@microsoft/microsoft-graph-client": [
            "../graph"
        ],
    },
    "files": [],
    "references": [
        {
            "path": "../serviceLib/tsconfig.cjs.json"
        },
        {
            "path": "../graph/tsconfig.cjs.json"
        }
    ]
}